package Days41;
import java.util.Scanner;
public class Main {
    public static void main (String[] args) {
        Scanner yu = new Scanner(System.in);
        int sum = 0;
        
        int n = yu.nextInt();
        for(int a = 1; a <= n; a++){
            System.out.print("Masukkan inputan " + a + " = ");
            int j = yu.nextInt();
            sum += j;
            
        }
        float avg = sum / n;
        System.out.println("Average = " + avg);
        System.out.println("SUM = " + sum);
        
  }}

        
        
        
        

    public static void main (String[] args) {
        
        int a;
        a = 7;
        
        a += 8 ;
        a -= 2 ;
        a *= 4 ;
        a /= 2 ;
        a %= 9 ;
        
        System.out.println(a);
        
        }
            
        }
